{
    "extends": "@tsconfig/strictest/tsconfig.json",
    "compilerOptions": {
        "esModuleInterop": false,
        "module": "Node16",
        "moduleResolution": "Node16",
        "target": "ES2021",
        "outDir": "out",
        "noEmit": false,
        "lib": ["ES2021"],
        "sourceMap": true,
        "rootDir": ".",
        "newLine": "lf",
    },
    "exclude": ["node_modules"]
}
